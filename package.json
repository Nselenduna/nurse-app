{
  "name": "nurse-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "check-naming": "node ./scripts/rename-files.js",
    "rename-files": "node ./scripts/rename-files.js --apply",
    "check-deps": "node ./scripts/check-dependencies.js",
    "audit-deps": "npm audit && npm outdated",
    "security-scan": "node ./scripts/security-scan.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "validate": "npm run lint && npm run test && npm run check-naming"
  },
  "dependencies": {
