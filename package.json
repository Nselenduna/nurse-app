    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.4",
    "expo-secure-store": "~12.8.1",
    "expo-sharing": "~11.10.0",
    "expo-speech": "^13.1.7",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
